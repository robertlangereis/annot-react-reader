        <target>
            <fragment start="OEBPS/xhtml/Broo_9780679645047_epub3_itr_r1.xhtml#point(/1/4/4/82/16/1:315)" end="OEBPS/xhtml/Broo_9780679645047_epub3_itr_r1.xhtml#point(/1/4/4/82/16/1:344)" progress="0.0574018">
